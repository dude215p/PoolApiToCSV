<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
  <appSettings>
    <add key="api_ahash" value="http://www.ahashpool.com/api/walletEx?address=123ABCiPhJFMsyivKW3LYOURWALLETHERE"/>     <!-- for each api address -->
    <add key="api_href" value="http://pool.hashrefinery.com/api/walletEx?address=123ABCiPhJFMsyivKW3LYOURWALLETHERE"/>
    <add key="api_zpool" value="http://www.zpool.ca/api/walletEx?address=123ABCiPhJFMsyivKW3LYOURWALLETHERE"/>
    <add key="measure_ahash_t" value="total_earned"/>                                                                   <!-- gets each measure, by second part of name (eg ahash href zpool)-->
    <add key="measure_href_t" value="total"/>
    <add key="measure_zpool_t" value="total"/>
    <add key="csvFileName" value = "pools.csv"/>
    <add key="IntervalMinutes" value = "2"/>   <!-- can use decimals (eg. 0.1), but some pools might not want to respond to the same IP very fast-->
    <add key="ErrorLog" value = "error.log"/>
  </appSettings>
</configuration>
